{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"no-var-requires": false,
		"no-string-throw": false,
		"quotemark": [
			true,
			"single"
		],
		"semicolon": [
			true
		],
		"trailing-comma": false,
		"arrow-parens": false,
		"array-type": false
	},
	"rulesDirectory": []
}